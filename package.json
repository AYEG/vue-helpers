{
  "name": "@laura-wert/vue-helpers",
  "version": "2.1.0",
  "description": "",
  "productName": "",
  "license": "MIT",
  "author": " ",
  "main": "dist/index.umd.min.js",
  "typings": "lib/src/index.typings.d.ts",
  "bin": {
    "laura-wert-cli": "bin/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn lint && yarn clean && yarn build:ts && yarn build:main",
    "build:ts": "tsc -p . --declaration --emitDeclarationOnly --allowJs false --noImplicitAny false && rm -rf ./lib/test",
    "build:main": "vue-cli-service build --target lib --name org/index src/index.js && mv ./dist/org/index.umd.min.js ./dist/ && rm -rf ./dist/org && rm -rf ./dist/demo.html",
    "clean": "rimraf ./dist && rimraf ./lib",
    "release": "yarn git-clean-status &&  yarn build && yarn publish",
    "lint": "vue-cli-service lint --fix && ./bin/cli.js tslint",
    "test:unit": "vue-cli-service test:unit --include tests/unit/setup.js tests/unit/",
    "test:unit-s": "vue-cli-service test:unit --include tests/unit/setup.js",
    "test-ci": "yarn cover && cat ./coverage/lcov.info | yarn codacy-coverage",
    "cover": "cross-env NODE_ENV=coverage nyc --reporter=lcovonly vue-cli-service test:unit tests/unit/"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@laura-wert/git-clean-status": "^1.0.0",
    "@laura-wert/vue-test-helpers": "^2.5.4",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.12",
    "@types/sinon": "^7.0.13",
    "@types/sinon-chai": "^3.2.2",
    "@vue/cli-plugin-babel": "3.9.2",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-plugin-typescript": "^3.9.0",
    "@vue/cli-plugin-unit-mocha": "^3.9.0",
    "@vue/cli-service": "^3.9.2",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "axios": "^0.19.0",
    "babel-plugin-istanbul": "^5.1.0",
    "chai": "^4.2.0",
    "codacy-coverage": "^3.4.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-vue": "^5.2.3",
    "fork-ts-checker-webpack-plugin": "^1.4.2",
    "lint-staged": "^9.2.0",
    "nyc": "^14.1.1",
    "quasar": "^1.0.5",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typescript": "^3.5.3",
    "vee-validate": "^2.2.12",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.2.1",
    "vue-template-compiler": "^2.6.10"
  },
  "peerDependencies": {
    "quasar": ">=1.0.5",
    "vue": ">=2.5.16"
  },
  "resolutions": {
    "jsdom": "14.0.0"
  },
  "files": [
    "package.json",
    "bin/",
    "build-helpers/",
    "dist/",
    "lib/"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "vue-cli-service lint",
      "laura-wert-cli tslint",
      "git add"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "include": [
      "src/**/*.{js,ts,vue}"
    ],
    "reporter": [
      "lcov"
    ],
    "extension": [
      ".js",
      ".ts",
      ".vue"
    ],
    "cache": true,
    "all": true
  },
  "dependencies": {}
}
